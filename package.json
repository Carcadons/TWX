{
  "name": "twx-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "tsx watch server/api.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "next build",
    "build:api": "tsc server/api.ts --outDir dist",
    "start": "next start",
    "start:api": "node dist/api.js",
    "start:full": "concurrently \"npm run start\" \"npm run start:api\""
  },
  "dependencies": {
    "@speckle/viewer": "^2.25.4",
    "next": "^15.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "22.15.30",
    "@types/react": "19.1.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "typescript": "5.8.3",
    "tsx": "^4.19.4",
    "concurrently": "^9.1.2"
  }
}